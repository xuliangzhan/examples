    "clear": "node build/clear.js",
    "deploy": "npm install && npm run deploy:project1 && npm run deploy:project2 && npm run deploy:project3",
    "deploy:project1": "node build/build.js zip=project1 name=project1 && node deploy/deploy name=project1",
    "deploy:project2": "node build/build.js zip=project2 name=project2 && node deploy/deploy name=project2",
    "deploy:project3": "node build/build.js zip=project3 name=project3 && node deploy/deploy name=project3",
